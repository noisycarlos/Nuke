newPosx = nuke.selectedNode().xpos()
newPosy = nuke.selectedNode().ypos()
newfile = nuke.selectedNode()["file"].value()
newNode = nuke.nodes.Read(file = newfile)

newNode.setXpos(newPosx+100)
newNode.setYpos(newPosy)


